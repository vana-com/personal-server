{
  "name": "@opendatalabs/personal-server-ts-server",
  "version": "0.0.1",
  "description": "Hono HTTP server for the Vana Personal Server â€” routes, middleware, composition root",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vana-com/personal-server-ts.git",
    "directory": "packages/server"
  },
  "homepage": "https://github.com/vana-com/personal-server-ts#readme",
  "bugs": {
    "url": "https://github.com/vana-com/personal-server-ts/issues"
  },
  "type": "module",
  "main": "./dist/api.js",
  "types": "./dist/api.d.ts",
  "exports": {
    ".": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js"
    },
    "./runtime": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "copy-ui": "mkdir -p dist/ui && cp src/ui/index.html dist/ui/index.html",
    "build": "tsc --build && npm run copy-ui",
    "start": "node dist/index.js",
    "dev": "node --watch dist/index.js"
  },
  "dependencies": {
    "@opendatalabs/personal-server-ts-core": "*",
    "hono": "^4.12.2",
    "@hono/node-server": "^1.14.0"
  }
}
